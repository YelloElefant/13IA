   "version": "0.2.0",
   "configurations": [{
         "name": ".NET Core Launch (console)",
         "type": "coreclr",
         "request": "launch",
         "preLaunchTask": "build",
         "program": "${workspaceFolder}/DataEntry/bin/Debug/net7.0/DataEntry.dll",
         "args": ["${workspaceFolder}/DataEntry/NEW.csv", "${workspaceFolder}/DataEntry/OLD.html"],
         "cwd": "${workspaceFolder}/DataEntry",
         "console": "internalConsole",
         "stopAtEntry": false
      },
      {
         "name": ".NET Core Attach",
         "type": "coreclr",
         "request": "attach"
      }
   ]
}